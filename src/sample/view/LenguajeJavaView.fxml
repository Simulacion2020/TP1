<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.AleatorioJavaController">
<children>
    <GridPane fx:id="root" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="50.0" minHeight="36.0" prefHeight="36.0" valignment="TOP" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="488.0" vgrow="ALWAYS" />
            <RowConstraints maxHeight="100.0" minHeight="80.0" prefHeight="80.0" valignment="BOTTOM" vgrow="NEVER" />
        </rowConstraints>
        <children>
            <FlowPane prefHeight="200.0" prefWidth="200.0">
                <children>
                    <Label alignment="CENTER" prefHeight="16.0" prefWidth="100.0" text="Muestra:"/>
                    <TextField fx:id="muestraAleatorioJavaText" prefHeight="26.0" prefWidth="59.0"/>
                </children>
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </padding>
            </FlowPane>
        </children>
        <children>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
                <children>
                    <FlowPane prefHeight="52.0" prefWidth="374.0">
                        <children>
                            <Button mnemonicParsing="false" onAction="#handleGenerarAleatorioJava" text="Generar">
                                <FlowPane.margin>
                                    <Insets left="5.0" right="5.0"/>
                                </FlowPane.margin>
                            </Button>
                            <!--                                <Button disable="true" mnemonicParsing="false" text="Siguiente"/>-->
                            <Button fx:id="buttonChiAleatorioJava" disable="true" mnemonicParsing="false" text="prueba chiÂ²">
                                <FlowPane.margin>
                                    <Insets left="5.0" right="5.0"/>
                                </FlowPane.margin>
                            </Button>
                            <Button fx:id="buttonHistogramaAleatorioJava" disable="true" mnemonicParsing="false" text="Histograma"/>
                            <Label fx:id="camposInvalidosAleatorioJavaLabel" textFill="#e11704"/>
                        </children>
                    </FlowPane>
                    <FlowPane prefHeight="52.0" prefWidth="166.0">
                        <children>
                            <Button mnemonicParsing="false" onAction="#handleVolverButtonAction" text="Volver">
                                <FlowPane.margin>
                                    <Insets left="40.0"/>
                                </FlowPane.margin>
                            </Button>
                            <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleSalirButtonAction"
                                    text="Salir">
                                <FlowPane.margin>
                                    <Insets left="5.0"/>
                                </FlowPane.margin>
                            </Button>
                        </children>
                    </FlowPane>
                </children>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </HBox>
        </children>
    </GridPane>
</children>
<padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
</padding>
</AnchorPane>
