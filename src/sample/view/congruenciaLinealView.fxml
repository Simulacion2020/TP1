                        <!--                        <TextField fx:id="alinealtext" prefHeight="26.0" prefWidth="62.0"/>-->
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="33.0" text="k:"/>
                        <TextField fx:id="klinealtext" prefHeight="26.0" prefWidth="60.0"/>
